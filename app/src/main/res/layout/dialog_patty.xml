<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >
    <Button
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:id="@+id/pattyBbqIcon"
        android:background="@drawable/bbqicon"
        android:button ="@null"
        android:layout_margin = "20dp"
        />
    <Button
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:id="@+id/pattyChickenIcon"
        android:background="@drawable/chickenicon"
        android:button ="@null"
        android:layout_margin = "20dp"
        />
    <Button
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:id="@+id/pattyEggIcon"
        android:background="@drawable/eggsaladicon"
        android:button ="@null"
        android:layout_margin = "20dp"
        />
    <Button
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:id="@+id/pattyShrimpIcon"
        android:background="@drawable/shrimpicon"
        android:button ="@null"
        android:layout_margin = "20dp"
        />
    <Button
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:id="@+id/pattySteakIcon"
        android:background="@drawable/steakicon"
        android:button ="@null"
        android:layout_margin = "20dp"
        />
    <Button
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:id="@+id/endPattySelect"
        android:text="돌아가기"
        android:textSize="20dp"
        android:layout_marginTop="10dp"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/round_button"
        android:fontFamily="@font/rixinooariduriregular"
        android:textColor="@color/black"
        />


</LinearLayout>